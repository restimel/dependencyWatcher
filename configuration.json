{
	"rootFolders": ["main.js", "modules/*"],
	"fileFilter": {
		"whitelist": [{
			"pattern": "\\.js$"
		}]
	},
	"fileNameAdapter": [{
		"matcher": {"pattern": ".*/modules/(.*)$"},
		"output": "./modules/$1"
	}, {
		"matcher": {"pattern": "^\\./(?:\\./)+"},
		"output": "./"
	}],
	"types": [{
		"name": "modules",
		"matcher": {
			"pattern": "/modules/"
		},
		"color": "blue"
	}],
	"requireMatcher": [{
		"pattern": "require\\(['\"]((?:\\\\.|.)+?)['\"]\\)",
		"flags": "g"
	}],
	"requireNameAdapter": [{
		"matcher": {"pattern": "\\./(?!modules/)"},
		"output": "./modules/"
	}],
	"log": "test.log"
}